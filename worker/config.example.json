{
  "api_base_url": "http://localhost:3001/api",
  "api_token": "your_access_token_here",
  "worker_id": "backtest_worker_001",
  "poll_interval": 5.0,
  "log_level": "INFO",
  
  "data_strategy": {
    "use_cache": true,
    "cache_dir": "../cache",
    "source_priority": [
      "cache",
      "stock_access"
    ]
  },
  
  "data_sources": {
    "stock_access": {
      "enabled": true,
      "minute": false
    },
    "tushare": {
      "enabled": false,
      "token": "your_tushare_token_here"
    },
    "csv": {
      "enabled": false,
      "data_dir": "/data/stocks"
    }
  }
}
